server {
    listen 80 default_server;
    listen [::]:80 default_server;

    # listen 80
    # server_name pi pi.localnet

    location = /favicon.ico { access_log off; log_not_found off; }

    location /static/ {
        root /vagrant/static;
    }

    location / {
        include uwsgi_params;
        uwsgi_pass unix:/tmp/uwsgi.socket;
    }
}
